<p id="notice"><%= notice %></p>

<h1>Listing Services</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @masterservices.each do |masterservice| %>
      <tr>
        <td><%= masterservice.name %></td>
        <td><%= masterservice.description %></td>
        <td><%= link_to 'Show', masterservice %></td>
        <td><%= link_to 'Edit', edit_masterservice_path(masterservice) %></td>
        <td><%= link_to 'Destroy', masterservice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Masterservice', new_masterservice_path %>
